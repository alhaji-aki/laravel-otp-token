{
    "name": "alhaji-aki/laravel-otp-token",
    "description": "Create otp tokens for users on your application",
    "type": "library",
    "homepage": "https://github.com/alhaji-aki/laravel-otp-token",
    "license": "MIT",
    "keywords": [
        "framework",
        "laravel",
        "otp tokens"
    ],
    "authors": [
        {
            "name": "Abdul Kudus",
            "email": "abdulkudus2922@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "AlhajiAki\\OtpToken\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AlhajiAki\\OtpToken\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "AlhajiAki\\OtpToken\\OtpTokenServiceProvider"
            ],
            "aliases": {
                "OtpToken": "AlhajiAki\\OtpToken\\OtpToken"
            }
        }
    },
    "require": {
        "php": "^8.1|^8.2|^8.3|^8.4",
        "illuminate/contracts": "^10.0|^11.0|^12.0",
        "illuminate/database": "^10.0|^11.0|^12.0",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "ext-pdo_sqlite": "*",
        "laravel/pint": "^1.13",
        "larastan/larastan": "^2.7.0|^3.0",
        "mockery/mockery": "^1.3",
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^10.5|^11.5"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
